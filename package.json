      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "prettier --write"
    ],
    "__test__/**/*.(spec|test).js": [
      "jest --bail --coverage --findRelatedTests"
    ]
  }
